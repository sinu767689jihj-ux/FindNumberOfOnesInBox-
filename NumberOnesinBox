import java.util.Scanner;
public class FindNumberOfOnesInBox
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int[] box=new int[n];
        int i,count=0;
        for(i=0;i<n;i++)
        {
            box[i]=sc.nextInt();
        }
        //find the no.of ones in box logic implementation
        for(i=0;i<n;i++)
        {
            if(box[i]==1)
            {
                count+=1;
            }
        }
        System.out.println("Find number of ones in box:" + count);
        sc.close();
    }
}
